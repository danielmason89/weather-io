{
  "name": "weathercheck.io",
  "description": "WeatherCheck.io is an app to help with checking what the weather is in the area.",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n \"CLIENT,API\" -c \"cyan,green\" \"npm run dev:client\" \"npm run dev:api\"",
    "dev:api": "nodemon server.js",
    "dev:client": "vite --port 5173",
    "start": "node server.js",
    "build": "vite build",
    "preview": "npm run preview"
  },
